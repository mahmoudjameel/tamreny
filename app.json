{
  "expo": {
    "name": "tamrini",
    "slug": "tamrini",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/main-img/icon.png",
    "splash": {
      "image": "./app/assets/main-img/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tamrini.app",
      "buildNumber": "1.0.0",
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-2927383253903778~3597160507"
      },
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "We need to know your current location to find the near places. Do you allow us to use location service on this device ?"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/main-img/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.tamrini.app",
      "versionCode": 1,
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-2927383253903778~1737283929"
      },
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./app/assets/main-img/favicon.png"
    }
  }
}
